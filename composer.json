{
    "name": "hkt/psr7-asset",
    "description": "Managing assets via PSR-7",
    "license": "MIT",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        },
        {
            "name": "Paul M Jones",
            "email": "pmjones88@gmail.com"
        },
        {
            "name": "Bas Kamer",
            "email": "baskamer@gmail.com"
        }
    ],
    "config":            {
        "sort-packages":       true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "http-interop/http-factory": "^0.3",
        "psr/http-message": "^1.0",
        "webimpress/http-middleware-compatibility": "^0.1.4"
    },
    "require-dev": {
        "aura/di": "^3.0",
        "http-interop/http-factory-diactoros": "^0.3",
        "http-interop/http-middleware": "^0.5",
        "phpunit/phpunit": "5.7"
    },
    "suggest": {
        "http-interop/http-factory-diactoros": "Use zend framework zend-diactoros as the psr7 implementaion",
        "http-interop/http-factory-guzzle": "Use guzzlehttp as the psr7 implementaion"
    },
    "autoload": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@test"
        ],
        "test": "phpunit --colors=always"
    }
}
