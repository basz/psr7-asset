{
    "name": "aura/asset-bundle",
    "description": "Asset management for aura v2",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "aura/web-kernel":"2.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Asset_Bundle\\":"src/",
            "Aura\\Asset_Bundle\\_Config\\":"config/"
        }
    },
    "extra": {
        "aura": {
            "type": "project",
            "config": {
                "common": "Aura\\Asset_Bundle\\_Config\\Common"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
