{
    "name": "hkt/psr7-asset",
    "description": "Managing assets via PSR-7",
    "license": "MIT",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        },
        {
            "name": "Paul M Jones",
            "email": "pmjones88@gmail.com"
        },
        {
            "name": "Bas Kamer",
            "email": "baskamer@gmail.com"
        }
    ],
    "config":            {
        "sort-packages":       true
    },
    "require": {
        "php": "^7.3",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "aura/di": "^3.0",
        "laminas/laminas-diactoros": "^2.4",
        "phpunit/phpunit": "^9.4"
    },
    "suggest": {
        "laminas/laminas-diactoros": "Use laminas-diactoros as psr7 implementaion",
        "guzzlehttp/psr7": "Use guzzlehttp as psr7 implementaion"
    },
    "autoload": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@test"
        ],
        "test": "phpunit --colors=always"
    }
}
