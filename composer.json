{
    "name": "hkt/psr7-asset",
    "description": "Managing assets via PSR-7",
    "license": "MIT",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        },
        {
            "name": "Paul M Jones",
            "email": "pmjones88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/shadowhand/http-factory.git"
        }
    ],
    "require": {
        "psr/http-message":"~1.0",
        "psr/http-factory": "1.*@dev",
        "zendframework/zend-expressive-router": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
    "require-dev": {
        "zendframework/zend-diactoros":"~1.0"
    },
    "autoload": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"src/",
            "Hkt\\Psr7Asset\\_Config\\":"config/"
        }
    }
}
