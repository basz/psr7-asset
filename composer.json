{
    "name": "hkt/psr7-asset",
    "description": "Managing assets via PSR-7",
    "license": "MIT",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        },
        {
            "name": "Paul M Jones",
            "email": "pmjones88@gmail.com"
        }
    ],
    "require": {
        "php":"^5.6 || ^7.0",
        "psr/http-message":"^1.0",
        "zendframework/zend-diactoros":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"src/",
            "Hkt\\Psr7Asset\\_Config\\":"config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hkt\\Psr7Asset\\":"tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "5.7"
    }
}
